# For 1

#! /usr/bin/env python3
import sys
seq = sys.argv[1]

index = 0
for nucleotide in seq:
    print(seq[index])
    index += 1

# For 2

#! /usr/bin/env python3
import sys

num1 = int(sys.argv[1])
num2 = int(sys.argv[2])
num3 = int(sys.argv[3])
num4 = int(sys.argv[4])
num5 = int(sys.argv[5])

my_list = [num1, num2, num3, num4, num5]
my_new_list = []
for number in my_list:
    my_new_list.append(number ** 2)
print(my_new_list)

